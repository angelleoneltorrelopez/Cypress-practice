{
    "plugins": [],
    "source": {
        "include": [
            "./cypress",
            "./cypress.config.js"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "jsdocs/",
        "encoding": "utf8",
        "recurse": true
    }
}